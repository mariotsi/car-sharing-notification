{
  "extends": ["google", "eslint:recommended",
    "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
  ],
  "rules":{
    "require-jsdoc":"off",
    "max-len": [2, 120],
    "@typescript-eslint/ban-ts-comment": 0,
    "no-useless-escape": 0
}
}
