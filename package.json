{
  "name": "car-sharing-notification",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@types/node": "^8.0.17",
    "@types/request": "^2.0.0",
    "@types/request-promise": "^4.1.36",
    "@types/telebot": "^1.2",
    "@types/uuid": "^3.4.1",
    "date-fns": "1.x",
    "firebase-admin": "^10.0.1",
    "google-auth-library": "^0.10.0",
    "googleapis": "^18.0.0",
    "mongodb": "^4.3.1",
    "node-cron": "^1.1.3",
    "request": "^2.84.0",
    "request-promise": "^4.2.1",
    "telebot": "^1.2.3",
    "uuid": "^3.1.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv": "^14.2.0",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3"
  },
  "scripts": {
    "start": "node out/index.js",
    "start_local": "node -r dotenv/config out/index.js",
    "lint": "eslint --fix --ext .ts --ext .js src",
    "compile": "tsc",
    "prestart": "npm run compile",
    "prestart_local": "npm run compile",
    "prettier:ts": "prettier --single-quote --parser typescript --write src/*.ts src/**/*.ts",
    "prettier:json": "prettier --parser json --write *.json",
    "precommit2": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --single-quote --print-width 117 --parser typescript --write",
      "npm run lint",
      "git add"
    ],
    "*.jsonFake": [
      "prettier --parser json --write",
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariotsi/car-sharing-notification.git"
  },
  "author": "Simone Mariotti",
  "license": "ISC"
}
